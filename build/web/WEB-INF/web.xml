<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>processLogin</servlet-name>
        <servlet-class>processLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>availableChild</servlet-name>
        <servlet-class>availableChild</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>saveDetails</servlet-name>
        <servlet-class>saveDetails</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signup</servlet-name>
        <servlet-class>signup</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>processLogin</servlet-name>
        <url-pattern>/processLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>availableChild</servlet-name>
        <url-pattern>/availableChild</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>saveDetails</servlet-name>
        <url-pattern>/saveDetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signup</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
